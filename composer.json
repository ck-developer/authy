{
    "name": "ck-developer/authy",
    "type": "library",
    "description": "PHP client for Authy",
    "keywords": [
        "authy",
        "api",
        "authentication",
        "two-factor"
    ],
    "homepage": "https://github.com/ck-developer/authy",
    "license": "MIT",
    "authors": [
        {
            "name": "Claude Khedhiri",
            "email": "claude@khedhiri.com",
            "homepage": "http://www.khedhiri.com"
        }
    ],
    "require": {
        "php" : ">=5.3.0",
        "guzzlehttp/guzzle": "~6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.3",
        "scrutinizer/ocular": "~1.3",
        "fabpot/php-cs-fixer": "~1.9"
    },
    "autoload": {
        "psr-4": {
            "Authy\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Authy\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "fix": "./vendor/bin/php-cs-fixer fix"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0-dev"
        }
    }
}
